{
  "name": "event-sticker-hunt",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite --config app/vite.config.ts",
    "build": "vite build --config app/vite.config.ts",
    "preview": "vite preview --config app/vite.config.ts",
    "lint": "eslint \"app/src/**/*.{ts,tsx}\" --max-warnings=0",
    "format": "prettier --write \"{app,functions,scripts,docs}/**/*.{ts,tsx,js,jsx,json,md,css,cjs,cts,mts}\"",
    "setup:appwrite": "node scripts/setup-appwrite.mjs",
    "seed:stickers": "node scripts/seed-stickers.mjs",
    "functions:build": "npm run build --prefix functions/claimSticker"
  },
  "dependencies": {
    "@zxing/browser": "^0.1.5",
    "appwrite": "^18.1.1",
    "clsx": "^2.1.1",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-router-dom": "^6.28.0",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@types/node": "^22.9.3",
    "@types/react": "^18.3.12",
    "@types/react-dom": "^18.3.1",
    "@typescript-eslint/eslint-plugin": "^8.15.0",
    "@typescript-eslint/parser": "^8.15.0",
    "@vitejs/plugin-react": "^4.3.3",
    "autoprefixer": "^10.4.20",
    "eslint": "^9.14.0",
    "node-appwrite": "^13.0.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-react": "^7.37.2",
    "eslint-plugin-react-hooks": "^5.0.0",
    "globals": "^15.9.0",
    "postcss": "^8.5.6",
    "prettier": "3.6.2",
    "prettier-plugin-tailwindcss": "^0.6.14",
    "tailwindcss": "^3.4.14",
    "typescript": "^5.6.3",
    "vite": "^5.4.11"
  }
}
